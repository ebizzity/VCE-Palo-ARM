{
    "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {

        "VCE_LAN_Static_IP": {
            "defaultValue": "10.132.232.68",
            "type": "String"
        },
        "VCE_MGMT_Static_IP": {
            "defaultValue": "10.132.232.5",
            "type": "String"
        },
        "VCE_WAN_Static_IP": {
            "defaultValue": "10.132.232.20",
            "type": "String"
        },
        "VCE02_LAN_Static_IP": {
            "defaultValue": "10.132.232.69",
            "type": "String"
        },
        "VCE02_MGMT_Static_IP": {
            "defaultValue": "10.132.232.6",
            "type": "String"
        },
        "VCE02_WAN_Static_IP": {
            "defaultValue": "10.132.232.21",
            "type": "String"
        },
        "Palo_Alto_ILB-IP": {
            "type": "string",
            "metadata": {
                "description": "Normally is in the same subnet as the LAN NIC."
            }
        },
        "Palo_Alto_FW1-MGMT-IP": {
            "type": "string"
        },
        "Palo_Alto_FW1-WAN-IP": {
            "type": "string"
        },
        "Palo_Alto_FW1-LAN-IP": {
            "type": "string"
        },
        "Palo_Alto_FW2-MGMT-IP": {
            "type": "string"
        },
        "Palo_Alto_FW2-WAN-IP": {
            "type": "string"
        },
        "Palo_Alto_FW2-LAN-IP": {
            "type": "string"
        },
        "SN_HUB_TRANS1_Address_Space": {
            "defaultValue": "<TRANS1_Address Space>",
            "type": "string"
        },
        "SN_HUB_TRANS2_Address_Space": {
            "defaultValue": "<TRANS2_Address Space>",
            "type": "string"
        },
        "username": {
            "defaultValue": "azureadmin",
            "type": "string"
        },
        "SSH_Key": {
            "type": "SecureString"
        },

        "VCE-Vnet-RG": {
            "defaultValue": "<VNETRG>",
            "type": "String"
        },
        "VNETOption": {
            "type": "string",
            "defaultValue": "Create new VNET",
            "allowedValues": [
                "Create new VNET",
                "Use existing VNET"
            ],
            "metadata": {
                "description": "If creating a new VNET, anything not specified in the template will be deleted."
            }
        },
        "DeploymentOption": {
            "type": "string",
            "defaultValue": "Greenfield",
            "allowedValues": [
                "Greenfield",
                "Retrofit"
            ],
            "metadata": {
                "description": "Retrofit existing environment with missing pieces, or greenfield which deploys everything"
            }
        },
        "NVADeploymentOption": {
            "type": "string",
            "defaultValue": "Availability Set",
            "allowedValues": [
                "Availability Set",
                "Availability Zone"
            ],
            "metadata": {
                "description": "Leverage Availability Sets or Availability Zones for Deployment of NVAs"
            }
        },
        "VNETName": {
            "type": "string",
            "defaultValue": "<VCE-HUB-VNET>",
            "metadata": {
                "description": "Hub VNET name"
            }
        },
        "VN_HUB_Address_Space": {
            "defaultValue": "10.132.232.0/24",
            "type": "String"
        },
        "SN_HUB_LAN_Address_Space": {
            "defaultValue": "10.132.232.64/28",
            "type": "String"
        },
        "SN_HUB_WAN_Address_Space": {
            "defaultValue": "10.132.232.16/28",
            "type": "String"
        },
        "SN_HUB_MGMT_Address_Space": {
            "defaultValue": "10.132.232.0/28",
            "type": "String"
        },
        "NATIPWhitelistCIDR": {
            "defaultValue": "0.0.0.0/0",
            "type": "string"
        },
        "SiteCode": {
            "type": "string",
            "defaultValue": "<SITE_CODE>",
            "metadata": {
                "description": "Site Code for naming"
            }
        },
        "storageAccounts_diagnostics_name": {
            "type": "string",
            "defaultValue": "<DiagStorageacctName>",
            "metadata": {
                "description": "Storage account used for VM Diagnostics"
            }
        },
        "(opt.) AppendStringToResources": {
            "type": "string",
            "defaultValue": "",
            "metadata": {
                "description": "If the NAME and CONFIGURATION of the created LBs/NSGs/VMs/AVSET/VNET match an existing LBs/NSGs/VMs/AVSET/VNET in the SAME resource group, the template will its skip creation.  If the new resource NAME matches, but the resource CONFIGURATION does not, the template will fail or will overwrite the existing resource."
            }
        },
        "PublicLBAllowedPorts": {
            "type": "string",
            "defaultValue": "80, 443, 22, 3389",
            "metadata": {
                "description": "Ports allowed inbound from Public LB."
            }
        },
        "PaloMGMTNSGAllowFrom": {
            "type": "string",
            "defaultValue": "10.0.0.0/8",
            "metadata": {
                "description": "Range to allow connections to the Palo Management NIC."
            }
        },
        "paloPassword": {
            "type": "securestring",
            "metadata": {
                "description": "Initial Password for Palo Alto."
            }
        }
    },

//TODO: Add Parameters for VCE2 Static IPs, Username/Password for Palos

    "functions": [
    ],
    "variables": {
        "global_var_resource_group": "[resourceGroup().name]",
        "globalTemplateUriBase": "https://raw.githubusercontent.com/ebizzity/VCE-Palo-ARM/master",
        "global_var_appendedString": "[if(equals(parameters('(opt.) AppendStringToResources'), ''), '', parameters('(opt.) AppendStringToResources'))]",
        "global_vnet_name": "[if(equals(parameters('VNETOption'), 'Use existing VNET'), parameters('VNETName'), take(replace(concat('VN-',parameters('SiteCode'),'-HUB', variables('global_var_appendedString')), ' ', ''), 64))]",
        "global_vnet_resource_group": "[if(equals(parameters('VCE-Vnet-RG'), ''), variables('global_var_resource_group'), parameters('VCE-Vnet-RG'))]",
        "global_vnet_option": "[parameters('VNETOption')]",
        "global_SN_HUB_LAN_Name": "[take(replace(concat(parameters('SiteCode'),'-HUB-LAN'), ' ', ''), 80)]",
        "global_SN_HUB_WAN_Name": "[take(replace(concat(parameters('SiteCode'),'-HUB-WAN'), ' ', ''), 80)]",
        "global_SN_HUB_MGMT_Name": "[take(replace(concat(parameters('SiteCode'),'-HUB-MGMT'), ' ', ''), 80)]",
        "global_SN_HUB_TRANS1_Name": "[take(replace(concat(parameters('SiteCode'),'-HUB-TRANS1'), ' ', ''), 80)]",
        "global_SN_HUB_TRANS2_Name": "[take(replace(concat(parameters('SiteCode'),'-HUB-TRANS2'), ' ', ''), 80)]",
        "global_NIC_VCE_LAN_name": "[take(replace(concat('NIC-',parameters('SiteCode'),'-SV-VCE-LAN'), ' ', ''), 80)]",
        "global_NIC_VCE_WAN_name": "[take(replace(concat('NIC-',parameters('SiteCode'),'-SV-VCE-WAN'), ' ', ''), 80)]",
        "global_NIC_VCE_MGMT_name": "[take(replace(concat('NIC-',parameters('SiteCode'),'-SV-VCE-MGMT'), ' ', ''), 80)]",
        "global_RT_HUB_name": "[take(replace(concat('RT-',parameters('SiteCode'),'-HUB'), ' ', ''), 80)]",
        "global_networkSecurityGroups_NSG_HUB_WAN_name": "[take(replace(concat('NSG-',parameters('SiteCode'),'-HUB-WAN'), ' ', ''), 80)]",
        "global_networkSecurityGroups_NSG_HUB_MGMT_name": "[take(replace(concat('NSG-',parameters('SiteCode'),'-HUB-MGMT'), ' ', ''), 80)]",
        "global_publicIPAddresses_VCE01_ip_name": "[take(replace(concat(parameters('SiteCode'),'-SV-VCE-ip'), ' ', ''), 80)]",
        "global_virtualMachines_VCE01_name": "[take(replace(concat(parameters('SiteCode'),'-SV-VCE'), ' ', ''), 80)]",
        "global_storageAccounts_diagnostics_name": "[take(replace(parameters('storageAccounts_diagnostics_name'), ' ', ''), 80)]",
        "global_plb_name": "[take(replace(concat(parameters('SiteCode'),'-SV-PALO-PLB'), ' ', ''), 80)]",
        "global_ilb_name": "[take(replace(concat(parameters('SiteCode'),'-SV-PALO-ILB'), ' ', ''), 80)]",
        "global_paloASName": "[take(replace(concat(parameters('SiteCode'),'-SV-PALO-AS'), ' ', ''), 80)]",
        "global_paloAZName": "[take(replace(concat(parameters('SiteCode'),'-SV-PALO-AZ'), ' ', ''), 80)]",
        "global_palo-fw1-name": "[take(replace(concat(parameters('SiteCode'),'-SV-PALO-1'), ' ', ''), 80)]",
        "global_palo-fw2-name": "[take(replace(concat(parameters('SiteCode'),'-SV-PALO-2'), ' ', ''), 80)]",
        "global_paloNSGName": "[take(replace(concat('NSG-',parameters('SiteCode'),'HUB-PALOS'), ' ', ''), 80)]",
        "global_vm_adminUsername": "[parameters('username')]",
        "HubVNETTemplateUri": "[concat(variables('globalTemplateUriBase'),'/linkedtemplates/','hubvnet-deploy.json')]",
        "VCEgreenfieldtemplateUri": "[concat(variables('globalTemplateUriBase'),'/linkedtemplates/','vce-greenfield.json')]",
        "VCEretrofittemplateUri": "[concat(variables('globalTemplateUriBase'),'/linkedtemplates/','vce-retrofit.json')]",
        "PaloAStemplateUri": "[concat(variables('globalTemplateUriBase'),'/linkedtemplates/','palo-deploy-as.json')]",
        "PaloAZtemplateUri": "[concat(variables('globalTemplateUriBase'),'/linkedtemplates/','palo-deploy-az.json')]"

    },
    "resources": [

        {
            "comments": "Create Hub Virtual Network",
            "type": "Microsoft.Resources/deployments",
            "name": "CREATE_HUB_VNET",
            "apiVersion": "2019-07-01",
            "resourceGroup": "[variables('global_vnet_resource_group')]",
            "condition": "[equals(variables('global_vnet_option'), 'Create new VNET')]",
            "properties": {
                "mode": "Incremental",
                "templateLink": {
                    "uri": "[variables('HubVNETTemplateUri')]",
                    "contentVersion": "1.0.0.0"

                },
                "parameters": {
                    "VNET_NAME": {
                        "value": "[variables('global_vnet_name')]"
                    },
                    "address-space": {

                        "value": "[parameters('VN_HUB_Address_Space')]"
                    },
                    "LAN-subnet-name": {

                        "value": "[variables('global_SN_HUB_LAN_Name')]"
                    },
                    "LAN-subnet-address-space": {

                        "value": "[parameters('SN_HUB_LAN_Address_Space')]"
                    },
                    "WAN-subnet-name": {

                        "value": "[variables('global_SN_HUB_WAN_Name')]"
                    },
                    "WAN-subnet-address-space": {

                        "value": "[parameters('SN_HUB_WAN_Address_Space')]"
                    },
                    "MGMT-subnet-name": {

                        "value": "[variables('global_SN_HUB_MGMT_Name')]"
                    },
                    "MGMT-subnet-address-space": {

                        "value": "[parameters('SN_HUB_MGMT_Address_Space')]"
                    },
                    "TRANS1-subnet-name": {

                        "value": "[variables('global_SN_HUB_TRANS1_Name')]"
                    },
                    "TRANS1-address-space": {

                        "value": "[parameters('SN_HUB_TRANS1_Address_Space')]"
                    },
                    "TRANS2-subnet-name": {

                        "value": "[variables('global_SN_HUB_TRANS2_Name')]"
                    },
                    "TRANS2-address-space": {

                        "value": "[parameters('SN_HUB_TRANS2_Address_Space')]"
                    },
                    "networkSecurityGroups_NSG_HUB_MGMT_name": {

                        "value": "[variables('global_networkSecurityGroups_NSG_HUB_MGMT_name')]"
                    },
                    "networkSecurityGroups_NSG_HUB_WAN_name": {

                        "value": "[variables('global_networkSecurityGroups_NSG_HUB_WAN_name')]"
                    },
                    "NATIPWhitelistCIDR": {
                        "value": "[parameters('NATIPWhitelistCIDR')]"
                    },
                    "routeTables_RT_HUB_name": {
                        "value": "[variables('global_RT_HUB_name')]"
                    },
                    "VCE_LAN_Static_IP": {
                        "value": "[parameters('VCE_LAN_Static_IP')]"
                    }


                }
            }
        },    
        {
                "comments": "Create VCEs - Greenfield",
                "type": "Microsoft.Resources/deployments",
                "name": "CREATE_VCEs",
                "apiVersion": "2019-07-01",
                "resourceGroup": "[variables('global_vnet_resource_group')]",
                "condition": "[equals(parameters('DeploymentOption'), 'Greenfield')]",
                "properties": {
                    "mode": "Incremental",
                    "templateLink": {
                        "uri": "[variables('VCEgreenfieldtemplateUri')]",
                        "contentVersion": "1.0.0.0"

                    },
                "parameters": {
                    "global_NIC_VCE_LAN_name": {
                        "value": "[variables('global_NIC_VCE_LAN_name')]"
                    },
                    "global_vnet_name": {
                        "value": "[variables('global_vnet_name')]"
                    },
                    "global_SN_HUB_LAN_Name": {
                        "value": "[variables('global_SN_HUB_LAN_Name')]"
                    },
                    "VCE_LAN_Static_IP": {
                        "value": "[parameters('VCE_LAN_Static_IP')]"
                    },
                    "VCE02_LAN_Static_IP": {
                        "value": "[parameters('VCE02_LAN_Static_IP')]"
                    },
                    "global_NIC_VCE_MGMT_name": {
                        "value": "[variables('global_NIC_VCE_MGMT_name')]"
                    },
                    "global_SN_HUB_MGMT_Name": {
                        "value": "[variables('global_SN_HUB_MGMT_Name')]"
                    },
                    "global_networkSecurityGroups_NSG_HUB_MGMT_name": {
                        "value": "[variables('global_networkSecurityGroups_NSG_HUB_MGMT_name')]"
                    },
                    "global_publicIPAddresses_VCE01_ip_name": {
                        "value": "[variables('global_publicIPAddresses_VCE01_ip_name')]"
                    },
                    "global_SN_HUB_WAN_Name": {
                        "value": "[variables('global_SN_HUB_WAN_Name')]"
                    },
                    "VCE_MGMT_Static_IP": {
                        "value": "[parameters('VCE_MGMT_Static_IP')]"
                    },
                    "VCE_WAN_Static_IP": {
                        "value": "[parameters('VCE_WAN_Static_IP')]"
                    },
                    "VCE02_MGMT_Static_IP": {
                        "value": "[parameters('VCE02_MGMT_Static_IP')]"
                    },
                    "VCE02_WAN_Static_IP": {
                        "value": "[parameters('VCE02_WAN_Static_IP')]"
                    },
                    "global_NIC_VCE_WAN_name": {
                        "value": "[variables('global_NIC_VCE_WAN_name')]"
                    },
                    "global_storageAccounts_diagnostics_name": {
                        "value": "[variables('global_storageAccounts_diagnostics_name')]"
                    },
                    "global_virtualMachines_VCE01_name": {
                        "value": "[variables('global_virtualMachines_VCE01_name')]"
                    },
                    "SSH_Key": {
                        "value": "[parameters('SSH_Key')]"
                    },
                    "global_vnet_resource_group": {
                        "value": "[variables('global_vnet_resource_group')]"
                    }
                }
        
                  
            }
        }, 
        {
            "comments": "Create VCEs - Retrofit",
            "type": "Microsoft.Resources/deployments",
            "name": "CREATE_VCEs",
            "apiVersion": "2019-07-01",
            "resourceGroup": "[variables('global_vnet_resource_group')]",
            "condition": "[equals(parameters('DeploymentOption'), 'Retrofit')]",
            "dependsOn": [
                "CREATE_HUB_VNET"
            ],
            "properties": {
                "mode": "Incremental",
                "templateLink": {
                    "uri": "[variables('VCEretrofittemplateUri')]",
                    "contentVersion": "1.0.0.0"

                },
                "parameters": {

                    "global_NIC_VCE_LAN_name": {
                        "value": "[variables('global_NIC_VCE_LAN_name')]"
                    },
                    "global_vnet_name": {
                        "value": "[variables('global_vnet_name')]"
                    },
                    "global_SN_HUB_LAN_Name": {
                        "value": "[variables('global_SN_HUB_LAN_Name')]"
                    },
                    "VCE_LAN_Static_IP": {
                        "value": "[parameters('VCE_LAN_Static_IP')]"
                    },
                    "global_NIC_VCE_MGMT_name": {
                        "value": "[variables('global_NIC_VCE_MGMT_name')]"
                    },
                    "global_SN_HUB_MGMT_Name": {
                        "value": "[variables('global_SN_HUB_MGMT_Name')]"
                    },
                    "global_networkSecurityGroups_NSG_HUB_MGMT_name": {
