{
    "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "global_vnet_name": {
           "type": "string"
        },
        "global_vnet_resource_group": {
            "type": "string"
        },
        "global_nsg_resource_group": {
            "type": "string"
        },
        "TRANS1a-subnet-name": {
            "type": "string"
        },
        "TRANS1a-address-space": {
            "type": "string"
        },
        "TRANS2a-subnet-name": {
            "type": "string"
        },
        "TRANS2a-address-space": {
            "type": "string"
        },
        "Untrust-subnet-name": {
            "type": "string"
        },
        "Untrust-subnet-address-space": {
            "type": "string"
        },
        "networkSecurityGroups_NSG_HUB_TRANS1A_name": {
            "type": "string"
        },
        "networkSecurityGroups_NSG_HUB_TRANS2A_name": {
            "type": "string"
        },
        "networkSecurityGroups_NSG_HUB_UNTRUST_name": {
            "type": "string"
        },
        "NATGWtemplateUri": {
            "type": "string"
        },
        "publicprefixname": {
            "type": "string"
        },
        "natgwname": {
            "type": "string"
        }
        
    },
    "functions": [],
    "variables": {},
    "resources": [
        {
            "comments": "Create NATGW",
            "type": "Microsoft.Resources/deployments",
            "name": "CREATE_NATGW",
            "apiVersion": "2019-07-01",
            "resourceGroup": "[parameters('global_vnet_resource_group')]",
            "properties": {
                "mode": "Incremental",
                "templateLink": {
                    "uri": "[parameters('NATGWtemplateUri')]",
                    "contentVersion": "1.0.0.0"
                },
                "parameters": {

                    "publicprefixname": {
                        "value": "[parameters('publicprefixname')]"
                    },
                    "natgwname": {
                        "value": "[parameters('natgwname')]"
                    }
                }
            }
        },
        {
            "type": "Microsoft.Network/virtualNetworks/subnets",
            "apiVersion": "2019-09-01",
            "name": "[concat(parameters('global_vnet_name'), '/', parameters('TRANS1a-subnet-name'))]",
            "dependsOn": [

            ],
            
            "properties": {
                "addressPrefix": "[parameters('TRANS1a-address-space')]",
                "networkSecurityGroup": {
                    "id": "[resourceId(parameters('global_nsg_resource_group'), 'Microsoft.Network/networkSecurityGroups', parameters('networkSecurityGroups_NSG_HUB_TRANS1A_name'))]"
                },
                "serviceEndpoints": [
                    {
                        "service": "Microsoft.Storage",
                        "locations": [
                            "[resourceGroup().location]"
                        ]
                    }
                ],
                "delegations": [],
                "privateEndpointNetworkPolicies": "Enabled",
                "privateLinkServiceNetworkPolicies": "Enabled"
            }
        },
        {
            "type": "Microsoft.Network/virtualNetworks/subnets",
            "apiVersion": "2019-09-01",
            "name": "[concat(parameters('global_vnet_name'), '/', parameters('TRANS2a-subnet-name'))]",
            "dependsOn": [
                "[concat(resourceId(parameters('global_vnet_resource_group'), 'Microsoft.Network/virtualNetworks', parameters('global_vnet_name')), '/subnets/', parameters('TRANS1a-subnet-name'))]"
                
            ],
            
            "properties": {
                "addressPrefix": "[parameters('TRANS2a-address-space')]",
                "networkSecurityGroup": {
                    "id": "[resourceId(parameters('global_nsg_resource_group'), 'Microsoft.Network/networkSecurityGroups', parameters('networkSecurityGroups_NSG_HUB_TRANS2A_name'))]"
                },
                "serviceEndpoints": [
                    {
                        "service": "Microsoft.Storage",
                        "locations": [
                            "[resourceGroup().location]"
                        ]
                    }
                ],
                "delegations": [],
                "privateEndpointNetworkPolicies": "Enabled",
                "privateLinkServiceNetworkPolicies": "Enabled"
            }
        },
        {
            "type": "Microsoft.Network/virtualNetworks/subnets",
            "apiVersion": "2019-09-01",
            "name": "[concat(parameters('global_vnet_name'), '/', parameters('Untrust-subnet-name'))]",
            "dependsOn": [
                "[concat(resourceId(parameters('global_vnet_resource_group'), 'Microsoft.Network/virtualNetworks', parameters('global_vnet_name')), '/subnets/', parameters('TRANS2a-subnet-name'))]"
            ],
            
            "properties": {
                "addressPrefix": "[parameters('Untrust-subnet-address-space')]",
                "networkSecurityGroup": {
                    "id": "[resourceId(parameters('global_nsg_resource_group'), 'Microsoft.Network/networkSecurityGroups', parameters('networkSecurityGroups_NSG_HUB_UNTRUST_name'))]"
                },
                "serviceEndpoints": [
                    {
                        "service": "Microsoft.Storage",
                        "locations": [
                            "[resourceGroup().location]"
                        ]
                    }
                ],
                "delegations": [],
                "privateEndpointNetworkPolicies": "Enabled",
                "privateLinkServiceNetworkPolicies": "Enabled"
            }
        }

    ],
    "outputs": {}
}