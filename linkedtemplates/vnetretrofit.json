{
    "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "global_vnet_name": {
            "type": "string"
        },
        "global_vnet_resource_group": {
            "type": "string"
        },
        "TRANS1a-subnet-name": {
            "type": "string"
        },
        "TRANS1a-address-space": {
            "type": "string"
        },
        "TRANS2a-subnet-name": {
            "type": "string"
        },
        "TRANS2a-address-space": {
            "type": "string"
        },
        "Untrust-subnet-name": {
            "type": "string"
